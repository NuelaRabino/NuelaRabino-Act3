{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport ProductList from './components/ProductList.vue';\nimport ShoppingCart from './components/ShoppingCart.vue';\nexport default {\n  components: {\n    ProductList,\n    ShoppingCart\n  },\n  data() {\n    return {\n      products: [],\n      cart: [],\n      newProductName: '',\n      newProductPrice: ''\n    };\n  },\n  computed: {\n    total() {\n      return this.cart.reduce((acc, item) => acc + item.product.price * item.quantity, 0);\n    }\n  },\n  methods: {\n    addProduct() {\n      const name = this.newProductName.trim();\n      const price = parseFloat(this.newProductPrice);\n      if (name && price > 0) {\n        const newProduct = {\n          id: this.products.length + 1,\n          name: name,\n          price: price\n        };\n        this.products.push(newProduct);\n        this.newProductName = '';\n        this.newProductPrice = '';\n      }\n    },\n    addToCart(product) {\n      const index = this.cart.findIndex(item => item.product.id === product.id);\n      if (index !== -1) {\n        this.cart[index].quantity++;\n      } else {\n        this.cart.push({\n          product: product,\n          quantity: 1\n        });\n      }\n    },\n    removeFromCart(index) {\n      this.cart.splice(index, 1);\n    },\n    increaseQuantity(index) {\n      this.cart[index].quantity++;\n    },\n    decreaseQuantity(index) {\n      if (this.cart[index].quantity > 1) {\n        this.cart[index].quantity--;\n      }\n    }\n  }\n};","map":{"version":3,"names":["ProductList","ShoppingCart","components","data","products","cart","newProductName","newProductPrice","computed","total","reduce","acc","item","product","price","quantity","methods","addProduct","name","trim","parseFloat","newProduct","id","length","push","addToCart","index","findIndex","removeFromCart","splice","increaseQuantity","decreaseQuantity"],"sources":["C:\\Users\\Nuela\\my-laboratories\\src\\App.vue"],"sourcesContent":["<template>\n  <div>\n    <div class=\"add-product-container\">\n      <h2>Add Product:</h2>\n      <div class=\"input-container\">\n        <input type=\"text\" v-model=\"newProductName\" placeholder=\"Product Name\" class=\"input-field\">\n      </div>\n      <div class=\"input-container\">\n        <input type=\"number\" v-model=\"newProductPrice\" placeholder=\"Price\" class=\"input-field\">\n      </div>\n      <button @click=\"addProduct\" class=\"add-product-btn\">Create</button>\n    </div>\n\n    <product-list :products=\"products\" @add-to-cart=\"addToCart\"></product-list>\n\n    <shopping-cart :cart=\"cart\" @remove-from-cart=\"removeFromCart\" @increase-quantity=\"increaseQuantity\" @decrease-quantity=\"decreaseQuantity\"></shopping-cart>\n\n    <p>Total: Php {{ total }}</p>\n  </div>\n</template>\n\n<script>\nimport ProductList from './components/ProductList.vue';\nimport ShoppingCart from './components/ShoppingCart.vue';\n\nexport default {\n  components: {\n    ProductList,\n    ShoppingCart\n  },\n  data() {\n    return {\n      products: [],\n      cart: [],\n      newProductName: '',\n      newProductPrice: ''\n    };\n  },\n  computed: {\n    total() {\n      return this.cart.reduce((acc, item) => acc + (item.product.price * item.quantity), 0);\n    }\n  },\n  methods: {\n    addProduct() {\n      const name = this.newProductName.trim();\n      const price = parseFloat(this.newProductPrice);\n      \n      if (name && price > 0) {\n        const newProduct = {\n          id: this.products.length + 1,\n          name: name,\n          price: price\n        };\n        this.products.push(newProduct);\n        this.newProductName = '';\n        this.newProductPrice = '';\n      }\n    },\n    addToCart(product) {\n      const index = this.cart.findIndex(item => item.product.id === product.id);\n      if (index !== -1) {\n        this.cart[index].quantity++;\n      } else {\n        this.cart.push({ product: product, quantity: 1 });\n      }\n    },\n    removeFromCart(index) {\n      this.cart.splice(index, 1);\n    },\n    increaseQuantity(index) {\n      this.cart[index].quantity++;\n    },\n    decreaseQuantity(index) {\n      if (this.cart[index].quantity > 1) {\n        this.cart[index].quantity--;\n      }\n    }\n  }\n};\n</script>\n\n<style scoped>\n.add-product-container {\n  text-align: center;\n  margin-bottom: 20px;\n}\n\n.input-container {\n  margin-bottom: 10px;\n}\n\n.input-field {\n  padding: 10px;\n  border: 2px solid #3d6f49; /* Kulay ng border - darker green */\n  border-radius: 4px;\n  box-shadow: 0 0 5px rgba(0, 0, 0, 0.1); /* Shadow effect */\n}\n\n.add-product-btn {\n  background-color: #3d6f49; /* Darker green */\n  color: white;\n  border: none;\n  padding: 10px 20px;\n  font-size: 16px;\n  cursor: pointer;\n  border-radius: 4px;\n  transition-duration: 0.4s;\n}\n\n.add-product-btn:hover {\n  background-color: #2f5238; /* Darker shade ng green */\n}\n</style>"],"mappings":";AAsBA,OAAOA,WAAU,MAAO,8BAA8B;AACtD,OAAOC,YAAW,MAAO,+BAA+B;AAExD,eAAe;EACbC,UAAU,EAAE;IACVF,WAAW;IACXC;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,IAAI,EAAE,EAAE;MACRC,cAAc,EAAE,EAAE;MAClBC,eAAe,EAAE;IACnB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,KAAKA,CAAA,EAAG;MACN,OAAO,IAAI,CAACJ,IAAI,CAACK,MAAM,CAAC,CAACC,GAAG,EAAEC,IAAI,KAAKD,GAAE,GAAKC,IAAI,CAACC,OAAO,CAACC,KAAI,GAAIF,IAAI,CAACG,QAAS,EAAE,CAAC,CAAC;IACvF;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACX,MAAMC,IAAG,GAAI,IAAI,CAACZ,cAAc,CAACa,IAAI,CAAC,CAAC;MACvC,MAAML,KAAI,GAAIM,UAAU,CAAC,IAAI,CAACb,eAAe,CAAC;MAE9C,IAAIW,IAAG,IAAKJ,KAAI,GAAI,CAAC,EAAE;QACrB,MAAMO,UAAS,GAAI;UACjBC,EAAE,EAAE,IAAI,CAAClB,QAAQ,CAACmB,MAAK,GAAI,CAAC;UAC5BL,IAAI,EAAEA,IAAI;UACVJ,KAAK,EAAEA;QACT,CAAC;QACD,IAAI,CAACV,QAAQ,CAACoB,IAAI,CAACH,UAAU,CAAC;QAC9B,IAAI,CAACf,cAAa,GAAI,EAAE;QACxB,IAAI,CAACC,eAAc,GAAI,EAAE;MAC3B;IACF,CAAC;IACDkB,SAASA,CAACZ,OAAO,EAAE;MACjB,MAAMa,KAAI,GAAI,IAAI,CAACrB,IAAI,CAACsB,SAAS,CAACf,IAAG,IAAKA,IAAI,CAACC,OAAO,CAACS,EAAC,KAAMT,OAAO,CAACS